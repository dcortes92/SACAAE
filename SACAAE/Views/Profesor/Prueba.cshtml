@model IEnumerable<SACAAE.Models.Profesore>

@{
    ViewBag.Title = "Prueba";
    Layout = "~/Views/Shared/Privado.cshtml";
}

<h2>Prueba</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table>
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Nombre)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Plaza)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.HorasEnPropiedad)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Nombre)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Plaza)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.HorasEnPropiedad)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.ID }) |
            @Html.ActionLink("Details", "Details", new { id=item.ID }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.ID })
        </td>
    </tr>
}

</table>
